{
    "presets": ["next/babel"],
    "plugins":[
        [
            "styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false
            }
        ], 
        [
            "formatjs",
            {
              "idInterpolationPattern": "[sha512:contenthash:base64:6]",
              "ast": true
            }
        ]
    ]
}